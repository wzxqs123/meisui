<!--pages/paysuc/paysuc.wxml-->
<view class="paysuc-wrap">
  <view class="paysuc-tips">
    <view class="paysuc-payicon"></view>
    <text>{{ titles }}</text>
  </view>
  <view class="paysuc-order">
    <view class="paysuc-orderdel">
      <text>订单:</text>
      <view>{{ rechargeType }}</view>
    </view>
    <view class="paysuc-line"></view>
    <view class="paysuc-orderdel">
      <text>已支付:</text>
      <view>¥{{ rechargeMoney }}</view>
    </view>
  </view>
  <button class="paysuc-okbut" type="{{ butSet.type }}" open-type="{{ butSet.openType }}" app-parameter="{{ butSet.appParameter }}" hover-class="{{ butSet.hoverClass }}" binderror="{{ bindError }}">{{ butSet.con }}</button>
</view>